<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<TabPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.movie_management.Controller.MainController">
    <tabs>
        <Tab text="Movie">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">

                    <fx:include source="movies.fxml" />
                </AnchorPane>
            </content>
        </Tab>
        <Tab text="Category">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">

                    <fx:include source="categories.fxml" />
                </AnchorPane>
            </content>
        </Tab>
    </tabs>
</TabPane>